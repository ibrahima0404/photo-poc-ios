  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "build": "npm run clean && ionic build",
    "build:ios": "ionic capacitor build ios --no-open",
    "clean": "rimraf ./dist",
    "e2e": "ng e2e",
    "open:ios": "ionic capacitor copy ios && ionic capacitor open ios",
    "lint": "ng lint",
    "start": "ionic serve",
    "test": "ng test",
    "verify": "npm run verify:web && npm run verify:ios",
    "verify:ios": "cd ios/App && pod install && xcodebuild -workspace App.xcworkspace -scheme App -destination 'generic/platform=iOS' && cd ../..",
    "verify:web": "npm run build"
  },
